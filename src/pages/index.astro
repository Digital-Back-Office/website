---
import Layout from "../layouts/Layout.astro";
import VideoLayout from "../layouts/VideoLayout.astro";
import Hero from "../components/home/Hero.astro";
import Partners from "../components/home/Partners.astro";
import AISolutions from "../components/home/AISolutions.astro";
import About from "../components/home/About.astro";
import Features from "../components/home/Features.astro";
import Services from "../components/common/Services.astro";
import TechAndTestimonials from "../components/home/TechAndTestimonials.astro";
import Contact from "../components/common/Contact.astro";
import RoadMap from "../components/home/RoadMap.astro";

import '../styles/container.css'

// SEO-optimized metadata for homepage
const seoTitle = "AI Consulting Services & Enterprise Solutions - Digital Back Office";
const seoDescription = "Expert AI consulting services, AI transformation, generative AI, enterprise AI solutions, DevOps consulting, AWS, Azure, GCP, Databricks, and MLOps for startups.";
const seoKeywords = "AI Consulting Services, AI Transformation, Generative AI Company, Enterprise AI Solutions, DevOps Consultants, DevOps Consulting Services, AWS Consultants, Azure Consultants, Azure Consulting Services, AWS Consulting Services, Google Cloud Consultants, DevOps Experts, Azure Cloud Consultant, Azure Experts, AWS Experts, Databricks Consultants, Google Cloud Consulting Services, Google Cloud Experts, DevOps Consulting Solutions, MLOps for Startup";

---

<Layout 
	title={seoTitle}
	description={seoDescription}
	keywords={seoKeywords}
	canonical="/"
	preview_image="/preview.png"
>
	<main class="page-wrapper" data-pagefind-ignore>
		<VideoLayout src="/assets/videos/dbo-bg-video-home.mp4">
		<Hero />
		</VideoLayout>
		<Partners />
		<AISolutions />
		<About />
		<TechAndTestimonials />
		<Services />
		<RoadMap />
		<Features />
		<Contact />
	</main>
</Layout>
